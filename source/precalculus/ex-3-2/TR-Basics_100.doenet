<problem newNamespace>
   <setup>


    <select name="a">
      -60 -45 -30 0 30 45 60 120 135 150 180 210 225 240 300 315 330 360
    </select>
    <m name="expr">\tan($a^{\circ})</m>
    <math name="exprRad" simplify>tan($a pi/180)</math>
    <number name="solN">$exprRad</number>
    <math name="sol">
       <conditionalContent>
         <case condition="$exprRad = tan(0)">0</case> <case
          condition="$exprRad = tan(pi/6)"
        >sqrt(3)/3</case> <case condition="$exprRad = tan(pi/4)">1</case> <case
          condition="$exprRad = tan(pi/3)"
        >sqrt(3)</case> <case condition="$exprRad = tan(2pi/3)">-sqrt(3)</case> <case
          condition="$exprRad = tan(3pi/4)"
        >-1</case> <case condition="$exprRad = tan(5pi/6)">-sqrt(3)/3</case>
      </conditionalContent>
    </math>

  </setup>

  <!--testing--> <!-- <p>a = $a</p>
<p>expr = $expr</p>
<p>exprRad = $exprRad</p>
<p>sol = $sol</p>
<p>solN = $solN</p> -->
  <!--testing-->

  <p>
    Find $expr. Enter your answer as an exact value using square roots when necessary.
    Do not use a calculator to give a decimal approximation.
  </p>

  <p>
    <m>$expr = </m><answer name="ans1"> <mathInput name="userAns" /> <award
        name="correct"
        symbolicEquality
        expandOnCompare
      >
         <when> $userAns = $sol </when>
      </award> <award
        name="numerical"
        allowedErrorInNumbers="0.01"
        allowedErrorIsAbsolute
        credit="0.5"
      > <when> $userAns = $solN </when> </award> </answer>
  </p>

  <!--*********************************************
            Feedbacks
**********************************************-->
  <setup>
     <select name="correctFeedback" type="text">
       <option>Awesome!</option> <option>Great job!</option> <option>
        Fantastic!
      </option> <option>Nice work!</option> <option>Amazing!</option> <option>
        Excellent!
      </option> <option>Right on!</option> <option>Perfect!</option>
    </select>
  </setup>
  <feedback condition="$correct or $correct2">$correctFeedback</feedback>

  <feedback condition="$numerical">
    Your answer should be an integer or an exact rational number, not a decimal
    approximation.
  </feedback>

  <!--*********************************************
            Hint
**********************************************-->
  <!-- <hint name="helpSection"><title>Worked Example</title>

<p>Here is a similar worked example:</p> 
<example newNameSpace>

  
<p><alert><em>Solution.</em></alert></p>

</example>

  
</hint> -->

  <!--*********************************************
            Solution
**********************************************-->
  <solution> <asList>$sol</asList> </solution>
</problem>

